{
    "compilerOptions": {
      "module": "es6",
      "target": "es5",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node"
    }
  }